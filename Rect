
public class Rect {
	// 1
	private int width;
	private int height;
	
	// 2
	public Rect(int width, int height) {
		this.width = width;
		this.height = height;
	}
	
	// 3
	public void setWidth(int width) {
		this.width = width;
	}
	
	public int getWidth() {
		return width;
	}
	
	public void setHeight(int height) {
		this.height = height;
	}
	
	public int getHeight() {
		return height;
	}
	
	public int getArea() {
		return (this.width * this.height);
	}
}
