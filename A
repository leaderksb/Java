
public class A {
	
	private int x;
	private int y;
	
	public A() {
/*		x = 1;
		y = 2;		*/
		this(2);
	}
	
	public A(int x) {
//		this.x = this.y = x;
		this(x, x);
	}
	
	public A(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public void printXY() {
		System.out.println("[" + x + ", " + y + "]");
	}
}
