
public class StudentTest {

	public static void main(String[] args) {
		Student s1 = new Student("김수빈", 1, 3.3);
		System.out.println(s1);
		
		Student s2 = new Student("강민수", 1, 3.8);
		System.out.println(s2);
		
		Student s3 = new Student("정지석", 2, 4.5);
		System.out.println(s3);
		
		s1.setScore(4.0);
		
		double total = s1.getScore() + s2.getGrade() +s3.getScore();
		System.out.println("[ 학점 총합 : " + total +" ]");
	}

}
