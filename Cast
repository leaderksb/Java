
public class Cast {

	public static void main(String[] args) {
		int i = 10;
		int j = 3;
		double d = 12.3;
		int result = i/j;
		
		d = i;
		
		System.out.println(d);
		//정수가 실수형이 되었기때문에 값이 잘렸다

		i = (int)d;
		
		System.out.println(i);
		//잘라내고 값을 넣어줬다
		
		System.out.println(result);
		//정수 나누기는 정수자리만 출력한다
		
		System.out.println("1"+"1");
		System.out.println(1 + 1 + "1");
		//접합이 된다

	}

}
