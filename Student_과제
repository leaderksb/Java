
public class Student {
	
// 필드에 성명, 학년, 학점, 학과, 성별 선언
   private String name;
   private int grade;
   private double score;
   private String department;
   private char sex;
  
// 생성자 Generate Constructor using Fields로 생성
   public Student(String name, int grade, double score, String department, char sex) {
      super();
      this.name = name;
      this.grade = grade;
      this.score = score;
      this.department = department;
      this.sex = sex;
   }

// 메소드 Generate Getters and Setters로 생성
   public String getName() {
      return name;
   }

   public void setName(String name) {
      this.name = name;
   }

   public int getGrade() {
      return grade;
   }

   public void setGrade(int grade) {
      this.grade = grade;
   }

   public double getScore() {
      return score;
   }

   public void setScore(double score) {
      this.score = score;
   }

   public String getDepartment() {
      return department;
   }

   public void setDepartment(String department) {
      this.department = department;
   }

   public char getSex() {
      return sex;
   }

   public void setSex(char sex) {
      this.sex = sex;
   }

// 학생 정보 출력용 print() 정의
   public void print() {
	   System.out.println("[ 학생정보 : 성명=" + name + ", 학년=" + grade + ", 학점=" + score + ", 학과=" + department + ", 성별="
         + sex + " ]");
   }

}
