
public class Cartest {

	public static void main(String[] args) {
		Car bmw = new Car();
//		bmw.brand = "BMW";
		bmw.setBrand("BMW");
//		bmw.price = 100;
		bmw.setPrice(100);
		Car hd = new Car();
//		hd.brand = "현대";
		hd.setBrand("현대");
//		hd.price = 50;
		hd.setPrice(50);
		
		System.out.println(bmw.getBrand() + " : " + bmw.getPrice());
		System.out.println(hd.getBrand() + " : " + hd.getPrice());
	}

}
